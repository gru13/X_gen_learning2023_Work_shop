    "from PIL import Image\n",
    "\n",
    "# Open image file\n",
    "image = Image.open('image.jpg')\n",
    "\n",
    "# Convert image to black and white\n",
    "bw_image = image.convert('L')\n",
    "\n",
    "# Save black and white image\n",
    "bw_image.save('bw_image.jpg')\n"
   ]
